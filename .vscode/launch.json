	"version": "0.2.0",
	"configurations": [
		{
			"type": "autohotkey",
			"request": "launch",
			"name": "AutoHotkey Debugger",
			"program": "${workspaceFolder}\\${relativeFile}",
			"runtime_v1": "AutoHotkey.exe",
			"runtime_v2": "v2/AutoHotkey.exe",
			"runtime": "v2/AutoHotkey64.exe",
			"port": "9002-9010",
			"useOutputDebug": true,
			"stopOnEntry": false
		}

	]
}